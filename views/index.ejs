<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>

    <% include templates/header.ejs %>

    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <p><%= points %></p>
    <ul>
        <% for (var i = 0; i < 5; i++) { %>
            <li>Tuna</li>
        <% } %>
    </ul>

    <h3><%= videoData.categoryName %></h3>

    <ul>
        <% videoData.categories.forEach(function(item) { %>
            <li><%= item.categoryName %></li>
        <% }); %>
    </ul>
</body>
</html>
